@page "/settings"
@attribute [Authorize]

@code {
    [CascadingParameter] AppState AppState { get; set; }

    protected override Task OnInitAsync()
    {
        AppState.SetContentHeader("Settings", new List<BreadcrumbItem> {
                new BreadcrumbItem { Text = "Home", Route = "#" },
                new BreadcrumbItem { Text = "Settings", Route = "settings" }
        });
        return Task.CompletedTask;
    }
}